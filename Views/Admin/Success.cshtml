
@{
    Layout = null;
    if (Session["AdmId"] == null)
    {
        Response.Redirect("~/Employee/Login");
    }
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet">
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet" />

    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body>
    <div>

        <div class="container">
            <h4>Employees in Excel Sheet</h4>
        </div>
        <table class="table table-hover table-bordered">
            <tr style="background: #ffc107;">
                <th>Employee ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Project ID</th>
                <th>WON Number</th>
                <th>Project Details</th>
                <th>Asset ID</th>
                <th>Token ID</th>
                <th>Location</th>
                <th>Lan ID</th>

            </tr>



            @foreach (var e in ViewBag.emplo)
            {
                <tr>
                    <td>@e.EmpId</td>
                    <td>@e.FirstName</td>
                    <td>@e.LastName</td>
                    <td>@e.Email</td>
                    <td>@e.ProjectId</td>
                    <td>@e.WONNumber</td>
                    <td>@e.ProjectDetails</td>
                    <td>@e.AssetId</td>
                    <td>@e.TokenId</td>
                    <td>@e.Location</td>
                    <td>@e.LanId</td>
                </tr>
            }

        </table>


        <div class="container">
            <h4>Succesfully Added</h4>
        </div>
        <table class="table table-hover table-bordered">
            <tr style="background: #6fcf99;">
                <th>Employee ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Project ID</th>
                <th>WON Number</th>
                <th>Project Details</th>
                <th>Asset ID</th>
                <th>Token ID</th>
                <th>Location</th>
                <th>Lan ID</th>

            </tr>



            @foreach (var e in ViewBag.added)
            {
                <tr>
                    <td>@e.EmpId</td>
                    <td>@e.FirstName</td>
                    <td>@e.LastName</td>
                    <td>@e.Email</td>
                    <td>@e.ProjectId</td>
                    <td>@e.WONNumber</td>
                    <td>@e.ProjectDetails</td>
                    <td>@e.AssetId</td>
                    <td>@e.TokenId</td>
                    <td>@e.Location</td>
                    <td>@e.LanId</td>
                </tr>
            }

        </table>



        <div class="container">
            <h4>Already existing employees</h4>
        </div>
        <table class="table table-hover table-bordered">
            <tr style="background: #dc3545;">
                <th>Employee ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Project ID</th>
                <th>WON Number</th>
                <th>Project Details</th>
                <th>Asset ID</th>
                <th>Token ID</th>
                <th>Location</th>
                <th>Lan ID</th>

            </tr>



            @foreach (var e in ViewBag.exist)
            {
                <tr>
                    <td>@e.EmpId</td>
                    <td>@e.FirstName</td>
                    <td>@e.LastName</td>
                    <td>@e.Email</td>
                    <td>@e.ProjectId</td>
                    <td>@e.WONNumber</td>
                    <td>@e.ProjectDetails</td>
                    <td>@e.AssetId</td>
                    <td>@e.TokenId</td>
                    <td>@e.Location</td>
                    <td>@e.LanId</td>
                </tr>
            }

        </table>


    </div>
</body>
</html>
